func = ""

print("enter function")
func = raw_input()

funcLength = len(func)
funcArray = []

x = funcLength

for i in range(0, funcLength):
    x = x - 2
    funcArray.append(func[i:-x])
for i in range(0, funcLength):
    print(funcArray[i])



